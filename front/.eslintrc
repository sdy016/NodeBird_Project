{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "plugins": ["import", "react-hooks"],
  "rules": {
    "no-underscore-dangle": "off",
    "react/forbid-prop-types": 0,
    "object-curly-newline": 0,
    "linebreak-style":0,
    "react/jsx-filename-extension":0,
    "react/jsx-wrap-multilines":0,
    "no-console":0,
    "import/no-extraneous-dependencies" : 0,
    "no-alert":0,
    "consistent-return":0,
    "spaced-comment":0,
  }
}
